{
  "name": "tsp-go-wasm-browser-compare-algorithms",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.27",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.0",
    "@types/react-redux": "^7.1.24",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^7.2.8",
    "react-scripts": "5.0.1",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "npm run compile-go && npm run cp-worker && npm run cp-exec && react-scripts start",
    "build": "npm run compile-go && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compile-go": "cd wasm_modules && GOOS=js GOARCH=wasm go build -o ../public/gomodule.wasm",
    "cp-worker": "cp ./src/workers/myworker.worker.js ./public",
    "cp-exec": "cp ./src/wasm_modules/wasm_exec.js ./public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
